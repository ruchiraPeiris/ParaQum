

@model IEnumerable<ParaQum.Models.Order>


@{
    ViewBag.Title = "Orders";
}
<br />
<div id="page-wrapper">
    <div class="container-fluid">
        <!-- Page Heading -->
        <div class="row">
            <div class="col-lg-12">
                <h2 class="page-header">
                    Current Orders
                </h2>
                <ol class="breadcrumb">
                    <li>
                        <span class="glyphicon glyphicon-home"></span>  Home
                    </li>
                    <li>
                        <i class="fa fa-fw fa-bar-chart-o"></i> Create Order
                    </li>
                    <li class="active">
                        <span class="glyphicon glyphicon-list-alt"></span> Orders
                    </li>
                </ol>
                <br />
                @*<div id="RenderCreate">
                        @Html.Partial("Create", new ParaQum.Models.Bom())
                    </div>*@

                @*<h4>Registered Users</h4>*@
                <br />

                <div class="panel panel-primary">
                    <!-- Default panel contents -->
                    <div class="panel-heading">Orders</div>
                    <table class="table">
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.OrderId)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.OrderExcelFile)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Date)
                            </th>

                        </tr>

                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.OrderId)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.OrderExcelFile)
                                </td>

                                <td>
                                    @Html.DisplayFor(modelItem => item.Date)
                                </td>

                                <td>



                                    @*@Html.ActionLink("View", "ViewBom", "Orders")*@
                                    <a href="/Orders/ViewBom/@item.OrderId">View</a>
                                    <a href="/Orders/edit/@item.OrderId">Edit</a>
                                    <a href="/Orders/delete/@item.OrderId">Delete</a>
                                <td />
                            </tr>


                        }

                    </table>
                </div>

                @if (null != TempData["createmsg"])
                {
                    <script type="text/javascript">
                        alert("@TempData["createmsg"]");
                    </script>
                }


            </div>
        </div>


    </div>
</div>